    "def min_sort(x,y):\n",
    "    min = x\n",
    "    max = y\n",
    "    if x>y:\n",
    "        min = y\n",
    "        max = x\n",
    "    return min,max\n",
    "\n",
    "x = 0\n",
    "y = 0\n",
    "z = 0\n",
    "  \n",
    "x,y,z = input(\"输入x,y,z的值:\").split(\",\")\n",
    "x=float(x)\n",
    "y=float(y)\n",
    "z=float(z)\n",
    "x,y =min_sort(x,y)\n",
    "y,z =min_sort(y,z)\n",
    "x,z =min_sort(x,z)\n",
    "print(x,y,z)"
   ]
  }

   "source": [
    "a,b = input(\"请输入两个正整数，用\\\",\\\"隔开\").split(\",\")\n",
    "a = int(a)\n",
    "b = int(b)\n",
    "#辗转相除法\n",
    "while b !=0:\n",
    "    temp = a%b\n",
    "    a = b\n",
    "    b = temp\n",
    "print(\"最大公约数为：\",a,sep=\"\")\n",
    "\n"
   ]
